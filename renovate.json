{
  "extends": [
    "config:base"
  ],
  "timezone": "Asia/Tokyo",
  "schedule": [
    "every weekend"
  ],
  "labels": [
    "renovate"
  ],
  "ignoreDeps": [
    "@nuxtjs/composition-api"
  ],
  "minor": {
    "groupName": "minor all dependencies ",
    "automerge": true
  },
  "patch": {
    "groupName": "patch all dependencies ",
    "automerge": true
  },
  "lockFileMaintenance": {
    "enabled": true,
    "automerge": true
  }
}
